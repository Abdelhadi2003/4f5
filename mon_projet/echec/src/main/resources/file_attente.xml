<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml"
    fx:controller="echec.frontal.vues.VueFileAttente"
	styleClass="conteneur"
	id="vue-file-attente">
	<Label id="text"
	text="%detailPartie">
	</Label>
    

    <Button 
		fx:id="boutonOuvrirDetailPartie"
		text="%retournerMenu"
    	styleClass="gros-bouton"
		id="bouton-ouvrir-detailPartie">
	</Button>

	<Button
		fx:id="boutonAjouterPiecePerdu"
		text="%ajouterPiecePerdu"
		styleClass="gros-bouton"
		id="boutonAjouterPiecePerdu">
	</Button>

	<Label
		fx:id="labelMessage"
		styleClass="texte"
		id="label-message"
		text="[labelMessage]">
	</Label>
	
</VBox>